Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"now", " ", "define", " ", "shorthand"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Beta]\[Epsilon]", " ", "=", " ", 
     RowBox[{
      RowBox[{"\[Tau]Q", " ", "\[Alpha]", " ", "csSq"}], " ", "+", " ", 
